{
  "name": "nuxt-app",
  "description": "Application utilizing Sitecore JavaScript Services and Nuxt.js",
  "private": true,
  "config": {
    "appName": "sxastarter",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts",
    "start:connected": "nuxt dev"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.16.19",
    "constant-case": "^3.0.4",
    "graphql-let": "^0.18.6",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.6.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "@sitecore-jss/sitecore-jss-dev-tools": "^21.2.3",
    "@sitecore-jss/sitecore-jss-vue": "^21.2.2"
  }
}
